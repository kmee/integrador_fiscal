        <Parametro>
          <Nome>idFila</Nome>
          <!--int-->
          <Valor>{{ consulta.id_fila }}</Valor>
        </Parametro>
        <Parametro>
          <Nome>cnpj</Nome>
          <!--string-->
          <Valor>{{ consulta.cnpj }}</Valor>
        </Parametro>
      </Parametros>
    </Metodo>
  </Componente>
</Integrador>