<Integrador>
  <Identificador>
    <Valor>{{ numero_identificador }}</Valor>
  </Identificador>
  <Componente Nome="VFP-e">
    <Metodo Nome="RespostaFiscal">
      <Construtor>
        <Parametros>
          <Parametro>
            <Nome>chaveAcessoValidador</Nome>
            <!--String-->
            <Valor>{{ consulta.chave_acesso_validador }}</Valor>
          </Parametro>
        </Parametros>
      </Construtor>
      <Parametros>
        <Parametro>
          <Nome>idFila</Nome>
          <!--int-->
          <Valor>{{ consulta.id_fila }}</Valor>
        </Parametro>
        <Parametro>
          <Nome>ChaveAcesso</Nome>
          <!--string-->
          <Valor>{{ consulta.chave_acesso }}</Valor>
        </Parametro>
        <Parametro>
          <Nome>Nsu</Nome>
          <!--string-->
          <Valor>{{ consulta.nsu }}</Valor>
        </Parametro>
        <Parametro>
          <Nome>NumerodeAprovacao</Nome>
          <!--string-->
          <Valor>{{ consulta.numero_aprovacao }}</Valor>
        </Parametro>
        <Parametro>
          <Nome>Bandeira</Nome>
          <!--string-->
          <Valor>{{ consulta.bandeira }}</Valor>
        </Parametro>
        <Parametro>
          <Nome>Adquirente</Nome>
          <!--string-->
          <Valor>{{ consulta.adquirente }}</Valor>
        </Parametro>
        <Parametro>
          <Nome>Cnpj</Nome>
          <!--string-->
          <Valor>{{ consulta.cnpj }}</Valor>
        </Parametro>
        <Parametro>
          <Nome>ImpressaoFiscal</Nome>
          <!--string-->
          <Valor>{{ consulta.impressao_fiscal }}</Valor>
        </Parametro>
        <Parametro>
          <Nome>NumeroDocumento</Nome>
          <!--string-->
          <Valor>{{ consulta.numero_documento }}</Valor>
        </Parametro>
      </Parametros>
    </Metodo>
  </Componente>
</Integrador>
