          <!--string-->
          <Valor>{{ consulta.valor_pagamento }}</Valor>
        </Parametro>
        <Parametro>
          <Nome>IdFila</Nome>
          <!--string-->
          <Valor>{{ consulta.id_fila }}</Valor>
        </Parametro>
        <Parametro>
          <Nome>Tipo</Nome>
          <!--string-->
          <Valor>{{ consulta.tipo }}</Valor>
        </Parametro>
        <Parametro>
          <Nome>UltimosQuatroDigitos</Nome>
          <!--string-->
          <Valor>{{ consulta.ultimos_quatro_digitos }}</Valor>
